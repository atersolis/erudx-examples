{
	"compilerOptions": {
		"jsx": "react",
		"jsxFactory": "React.createElement",
		"target": "es5",
		"lib": ["es2017", "dom"],
		"esModuleInterop": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"baseUrl": ".",
		"paths": {
			"react": ["node_modules/preact/compat"],
			"react-dom": ["node_modules/preact/compat"]
		}
	}
}
